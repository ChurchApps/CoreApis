{
  "name": "coreapis",
  "version": "1.0.0",
  "description": "Core APIs modular monolith for church management system",
  "main": "dist/index.js",
  "scripts": {
    "initdb": "ts-node tools/initdb.ts",
    "initdb:membership": "ts-node tools/initdb.ts --module=membership",
    "initdb:attendance": "ts-node tools/initdb.ts --module=attendance",
    "initdb:content": "ts-node tools/initdb.ts --module=content",
    "initdb:giving": "ts-node tools/initdb.ts --module=giving",
    "initdb:messaging": "ts-node tools/initdb.ts --module=messaging",
    "initdb:doing": "ts-node tools/initdb.ts --module=doing",
    "reset-db": "ts-node tools/initdb.ts --reset",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "clean": "rimraf dist",
    "lint": "eslint . --fix && prettier --write src/**/*.ts",
    "lint:check": "eslint .",
    "tsc": "tsc",
    "copy-assets": "copyfiles -u 1 src/**/*.html src/**/*.css dist/",
    "build-ignore-errors": "npm run clean && (npx tsc || true) && npm run copy-assets",
    "build": "npm run clean && npx tsc --noEmitOnError false && npm run copy-assets",
    "build-layer": "rm -rf layer && mkdir -p layer/nodejs && cp tools/layer-package.json layer/nodejs/package.json && cd layer/nodejs && npm install --production",
    "rebuild-layer": "npm run clean-layer && npm run build-layer",
    "clean-layer": "rm -rf layer",
    "serverless-local": "serverless offline --stage dev --httpPort 8084",
    "deploy-demo": "npm run build && serverless deploy --stage demo",
    "deploy-staging": "npm run build && serverless deploy --stage staging",
    "deploy-prod": "npm run build && serverless deploy --stage prod",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "migrate-env": "node tools/migrate-env.js"
  },
  "keywords": [
    "church",
    "api",
    "aws",
    "lambda"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.686.0",
    "@aws-sdk/client-cloudwatch": "^3.686.0",
    "@aws-sdk/client-lambda": "^3.686.0",
    "@aws-sdk/client-polly": "^3.864.0",
    "@aws-sdk/client-s3": "^3.686.0",
    "@aws-sdk/client-ses": "^3.686.0",
    "@aws-sdk/client-sqs": "^3.686.0",
    "@aws-sdk/client-ssm": "^3.686.0",
    "@aws-sdk/lib-storage": "^3.686.0",
    "@churchapps/apihelper": "^0.1.9",
    "@codegenie/serverless-express": "^4.15.0",
    "@google-cloud/speech": "^6.7.0",
    "@google-cloud/storage": "^7.14.0",
    "@hubspot/api-client": "^13.1.0",
    "@stripe/stripe-js": "^4.10.0",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "csv-parse": "^5.6.0",
    "dotenv": "^16.4.5",
    "expo-server-sdk": "^3.15.0",
    "express": "^4.21.1",
    "express-fileupload": "^1.5.1",
    "express-validator": "^7.2.1",
    "firebase-admin": "^13.0.0",
    "fs-extra": "^11.2.0",
    "google-auth-library": "^9.15.0",
    "html-to-text": "^9.0.5",
    "ics": "^3.8.1",
    "inversify": "^6.0.3",
    "inversify-binding-decorators": "^4.0.0",
    "inversify-express-utils": "^6.4.6",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.46",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.11.4",
    "node-fetch": "^2.7.0",
    "node-geocoder": "^4.4.1",
    "nodemailer": "^6.9.16",
    "oauth": "^0.10.2",
    "openai": "^4.72.0",
    "pexels": "^1.4.0",
    "reflect-metadata": "^0.2.2",
    "request": "^2.88.2",
    "stripe": "^17.4.0",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.3",
    "ws": "^8.18.0",
    "xml2js": "^0.6.2",
    "youtube-captions-scraper": "^2.0.3"
  },
  "devDependencies": {
